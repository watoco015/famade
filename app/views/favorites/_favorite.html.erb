 <% if user_signed_in? %>
    <% if art.favorited_by?(current_user) %> 
        <p>
        <%=button_to image_tag("favorite_b.png"), art_favorites_path(art.id), method: :delete , class: "fas fa-favorite"%>
        </p>
    <% else %>
        <p>
        <%=button_to image_tag("favorite_w.png"), art_favorites_path(art.id), method: :post , class: "far fa-favorite"%>
        </p>
    <% end %>
  <% end %>
